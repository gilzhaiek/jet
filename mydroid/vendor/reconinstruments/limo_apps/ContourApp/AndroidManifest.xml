<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="6773"
	  android:versionName="3.1_aug22"
	  package="com.contour.connect"
	  xmlns:android="http://schemas.android.com/apk/res/android">
	<permission android:name="com.contour.connect.SERVICE_CONTROLLER"/>
