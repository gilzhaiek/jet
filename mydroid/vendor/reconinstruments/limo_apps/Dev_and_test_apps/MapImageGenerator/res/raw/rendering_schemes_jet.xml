<?xml version="1.0" encoding="utf-8"?>

<rendering_scheme>
    
<!-- mpp = meters per pixel -->
<file_id id="jet Sep 20 2013 A"/>
<general>
	<pan_over_text text_size="20" text_rgb_color="ffffff" text_outline_width="2" text_outline_rgb_color="4d4d4d" />
	<pan_over_box bkgd_rgb_color="000000" bkgd_alpha="102" outline_width="2" outline_rgb_color="404040" outline_alpha="0" />
    <grid bkgd_grey_color="80" line_gray_color="90" line_alpha="255" line_width="1" />
 <!-- the following are not implemented yet -->
    <map_bg_color rgb_color="ffffff" alpha="255" />
    <vignette_file image="explore_btn" />
    <trail_outlines enabled="0" />
    <area_outlines enabled="0" />
</general>
<messages>
    <message state="startup"				message="Starting map..." />
    <message state="scheme_error" 			message="ERROR accessing rendering schemes..." />
    <message state="out_of_memory" 			message="Out of memory. Can not load map." />
    <message state="waiting_for_location" 	message="Waiting for GPS fix..." />
    <message state="loading_data" 			message="Loading resort..." />
    <message state="cache_data_loaded"		message="disabled" />
    <message state="no_data"		 		message="No map available" />
    <message state="closest_resort"	 		message="Nearest resort:" />
    <message state="error_loading_data" 	message="ERROR loading map" />
</messages>
<rates>
    <rate item="frame_rate"		rate="30.0" />
    <rate item="pan_rate"		rate="0.0002" />
    <rate item="rotation_rate"	rate="3.0" />
    <rate item="scale_rate"		rate="0.2" />
</rates>
<zoom_levels>
<!-- app code currently limited to max 4 zoom levels -->
    <zoom_level zoom_level="1" default_mpp="0.25" min_mpp="0.25" />
    <zoom_level zoom_level="2" snap_to_mpp="0.5" />
    <zoom_level zoom_level="3" snap_to_mpp="1.0" />
    <zoom_level zoom_level="4" snap_to_mpp="2.0" />
</zoom_levels>
<scale_ranges>
<!-- app code currently limited to max 4 scale ranges -->
    <scale_range scale_range="1" from_mpp="0.0" to_mpp="0.4" />
    <scale_range scale_range="2" from_mpp="0.4" to_mpp="0.8" />
    <scale_range scale_range="3" from_mpp="0.8" to_mpp="1.8" />
    <scale_range scale_range="4" from_mpp="1.8" to_mpp="1000.0" />
</scale_ranges>
