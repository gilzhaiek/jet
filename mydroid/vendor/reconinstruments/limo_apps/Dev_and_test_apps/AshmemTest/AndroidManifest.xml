<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reconinstruments.ashmemtest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="16" />
    <application
        android:icon="@drawable/ic_launcher"
        android:persistent="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:process="com.reconinstruments.ashmemtest.main">
        <activity
            android:name=".MainActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".ResultService"
            android:process="com.reconinstruments.ashmemtest.result" />

        <uses-library android:name="com.reconinstruments.lib" />
    </application>

</manifest>
