<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reconinstruments.jump"
    android:versionCode="6607"
    android:versionName="1.3.11" >

    <uses-sdk android:minSdkVersion="10" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <application android:icon="@drawable/jumpicon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        
        <activity
            android:label="@string/app_name"
            android:name=".ReconJumpActivity"
            android:screenOrientation="landscape" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
             <intent-filter>
				<action android:name="RECON_JUMP" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
        </activity>
        
        <activity android:name=".JumpHistoryActivity"
            android:screenOrientation="landscape" />
        
        <service android:exported="true"
            android:name="com.reconinstruments.applauncher.transcend.ReconTranscendService"
            android:process=":remote" />
    </application>

</manifest>