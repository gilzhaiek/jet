<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.reconinstruments.navigation"
      android:versionCode="6607"
      android:versionName="1.3.11">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>    
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="false">
        <activity android:name=".navigation.NavigationActivity"
                  android:label="@string/app_name"
                  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
                  android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
     
        <activity android:name=".navigation.PoiCategoryList" 
        		  android:label="@string/pick_poicategory_title"
        		  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
        		  android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.SHAPE_MAP" />
            </intent-filter>
        </activity>
        
        <!-- android:theme="@android:style/Theme.Dialog" -->
        <activity android:name=".navigation.PoiItemList" 
          		  android:label="Pick up a Point-of-Interests"          		  
          		  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
          		  android:screenOrientation="landscape" >  
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.SHAPE_MAP" />
            </intent-filter>
        </activity>
        
        <activity android:name=".navigation.NetworkDiagnosticList" 
        		  android:label="Pick up a Network for Diagnostics"
        		  android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.SHAPE_MAP" />
            </intent-filter>
        </activity>
        
        <activity android:name=".navigation.NetworkIsolatedNodeList" 
          		  android:label="Pick up a Isloated Node"
          		  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
          		  android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.SHAPE_MAP" />
            </intent-filter>
        </activity>
                
        <activity android:name=".navigation.datamanagement.CountryListActivity" 
        		  android:label="Select a Country"
        		  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
        		  android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.ResortAssets_INFO" />
            </intent-filter>
        </activity>
        
        <activity android:name=".navigation.datamanagement.RegionListActivity" 
          		  android:label="Pick up a Region"
          		  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
          		  android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.ResortAssets_INFO" />
            </intent-filter>
        </activity>
        
        <activity android:name=".navigation.datamanagement.ResortListActivity" 
        		  android:label="Select a Resort"
        		  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
        		  android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.ResortAssets_INFO" />
            </intent-filter>
        </activity>
        
        <service android:exported="true" android:name="com.reconinstruments.applauncher.transcend.ReconTranscendService" android:process=":remote">
   		</service>
        	
        
    </application>
</manifest> 