    <message state="error_initializing"		message="initialization issue..." />
    <message state="waiting_for_service"	message="LOADING MAP" />
    <message state="waiting_for_location" 	message="waiting for GPS fix..." />
    <message state="error_with_service"		message="problem with map service..." />
    <message state="map_data_unavailable" 	message="map data unavailable..." />
    <message state="out_of_memory" 			message="Out of memory. Can not load map." />
